{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint",
        "html"
    ],
    "rules": {
      "no-useless-constructor": "off",
      "@typescript-eslint/no-useless-constructor": ["error"]
    }
}
